{
    "name": "react-router-v7-template",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "react-router build",
        "dev": "react-router dev --host",
        "start:client": "vite preview --outDir ./build/client",
        "start:server": "react-router-serve ./build/server/index.js",
        "typecheck": "react-router typegen && tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "prepare": "husky"
    },
    "dependencies": {
        "@channel.io/channel-web-sdk-loader": "^2.0.2",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/modifiers": "^9.0.0",
        "@dnd-kit/sortable": "^10.0.0",
        "@emotion/css": "^11.13.5",
        "@emotion/is-prop-valid": "^1.4.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.8",
        "@mui/material": "^7.3.8",
        "@react-router/fs-routes": "^7.13.0",
        "@react-router/node": "^7.13.0",
        "@react-router/serve": "^7.13.0",
        "@sentry/react": "^10.39.0",
        "@tanstack/react-query": "^5.90.21",
        "@tanstack/react-query-devtools": "^5.91.3",
        "@tanstack/react-query-persist-client": "^5.90.22",
        "axios": "^1.13.5",
        "axios-mock-adapter": "^2.1.0",
        "canvas": "^3.2.1",
        "fast-glob": "^3.3.3",
        "framer-motion": "^12.34.2",
        "i18next": "^25.8.11",
        "i18next-browser-languagedetector": "^8.2.1",
        "ical-generator": "^10.0.0",
        "idb-keyval": "^6.2.2",
        "isbot": "^5.1.35",
        "loglevel": "^1.9.2",
        "mixpanel-browser": "^2.74.0",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-i18next": "^16.5.4",
        "react-intersection-observer": "^10.0.3",
        "react-router": "^7.13.0",
        "react-router-dom": "^7.13.0",
        "zod": "^4.3.6",
        "zustand": "^5.0.11"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@playwright/test": "^1.58.2",
        "@react-router/dev": "^7.13.0",
        "@sentry/vite-plugin": "^4.9.1",
        "@tanstack/eslint-plugin-query": "^5.91.4",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@testing-library/user-event": "^14.6.1",
        "@trivago/prettier-plugin-sort-imports": "^6.0.2",
        "@types/node": "^20",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.4",
        "dotenv-cli": "^11.0.0",
        "eslint": "^10.0.0",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^17.3.0",
        "happy-dom": "^20.6.3",
        "husky": "^9.1.7",
        "jsdom": "^28.1.0",
        "lint-staged": "^16.2.7",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.1",
        "vite-plugin-svgr": "^4.5.0",
        "vite-tsconfig-paths": "^6.1.1",
        "vitest": "^4.0.18"
    },
    "packageManager": "pnpm@10.7.1"
}
